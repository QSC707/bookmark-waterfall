{
  "manifest_version": 3,
  "name": "My Extension",
  "version": "1.0",
  "permissions": [
      "history",
      "bookmarks",
      "topSites",
      "favicon",
      "windows",
      "clipboardWrite"
  ],
  "chrome_url_overrides": {
      "newtab": "newtab.html"
  },
  "background": {
    "service_worker": "background.js"
  },
  "commands": {
    "toggle-bookmark-window": {
      "suggested_key": {
        "default": "Ctrl+Q",
        "mac": "Command+Q"
      },
      "description": "打开/关闭书签主页窗口"
    }
  },
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  }
}